{
  "schemaVersion": 1,
  "id": "illuminate",
  "name": "Illuminate",
  "description": "An API for Minecraft to allow for projecting images similar to real video projectors into the world.",
  "version": "1.0.0",
  "side": "universal",
  "dependencies": {
    "net.fabricmc.language.kotlin": {
      "version": ">=1.3.10-8"
    }
  },
  "entrypoints": {
    "main": [{ "adapter": "kotlin", "value": "therealfarfetchd.illuminate.Illuminate" }],
    "client": ["net.dblsaiko.illuminate.client.IlluminateClient::initializeClient"]
  },
  "mixins": [
    "illuminate-mixins-client.json"
  ]
}
